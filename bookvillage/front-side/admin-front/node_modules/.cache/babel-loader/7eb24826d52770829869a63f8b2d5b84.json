          .then(response => {\n                return response.data;\n            }).catch(error => {\n                console.log(error);\n            })\n    }\n\n    updateCustomer(customerObj) {\n        const userSeq = customerObj.userSeq\n        return $axiosInst\n            .put(`${this.host}/user/update/` + userSeq , customerObj)\n            .then( () => {\n                return;\n            }).catch( error => {\n                console.log(error);\n            })\n    }\n\n    deleteCustomer(userSeq){\n        return $axiosInst\n            .delete(`${this.host}/user/delete/` + userSeq)\n            .then( () => {\n                return;\n            }).catch( error => {\n                console.log(error);\n            })\n    }\n\n\n\n}"]}]}